{
  "name": "@skills-guard/sdk",
  "version": "0.1.0",
  "description": "Skills Guard SDK - Client Library for Agent Skills Security Scanner",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "skills-guard",
    "agent-skills",
    "security",
    "sdk",
    "client",
    "anthropic"
  ],
  "author": "Skills Guard Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skills-guard/skills-guard.git",
    "directory": "packages/sdk"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
